{
  "name": "askoufis.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "url": "https://github.com/askoufis/askoufis.github.io"
  },
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build:site": "pnpm --filter 'site' build",
    "dev": "pnpm --filter 'site' dev",
    "deploy:site": "pnpm --filter 'site' run deploy",
    "eslint": "eslint .",
    "lint": "pnpm run tsc && pnpm run eslint && pnpm format:check",
    "format": "eslint . --fix && prettier --write .",
    "format:check": "prettier --list-different .",
    "test": "echo 'no tests yet'",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "gh-pages": "^4.0.0",
    "eslint": "^8.23.1",
    "eslint-config-seek": "^10.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "packageManager": "pnpm@7.11.0",
  "volta": {
    "node": "16.17.0"
  }
}
